{
  "name": "flight-deck",
  "version": "0.8.3",
  "description": "Flight Deck is an opinionated starter project for Jekyll that uses modern front-end tooling.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "bundle exec gulp",
    "test": "bundle exec gulp test",
    "browsersync": "bundle exec gulp browsersync",
    "browsersync-production": "bundle exec gulp browsersync --jekyllEnv='production'",
    "build": "bundle exec gulp build --jekyllEnv='production'",
    "deploy": "bundle exec gulp deploy",
    "eslint": "bundle exec gulp eslint",
    "imagemin": "bundle exec gulp imagemin",
    "production": "bundle exec gulp --jekyllEnv='production'",
    "sass": "bundle exec gulp sass",
    "watch": "bundle exec gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PortsideIO/flightdeck.git"
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "Ginfuru",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PortsideIO/flightdeck/issues"
  },
  "homepage": "https://github.com/PortsideIO/flightdeck/",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "browser-sync": "^2.24.4",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rsync": "^0.0.8",
    "gulp-sass": "^4.0.1",
    "gulp-uglify": "^3.0.1",
    "gulp-watch": "^5.0.1",
    "imagemin-pngquant": "^6.0.0",
    "require-dir": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "webpack": "^4.16.5",
    "webpack-stream": "^5.1.0",
    "yargs": "^12.0.1"
  },
  "dependencies": {}
}
